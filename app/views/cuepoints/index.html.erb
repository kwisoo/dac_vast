<h1>キューポイント一覧</h1>
<% #TODO %>
<%= link_to "登録", new_cuepoint_path, class: "btn btn-primary" %>

    <table class="table">
      <thread>
        <tr>
          <th>id</th>
          <th>cue point</th>
          <th>create</th>
          <th>update</th>
        </tr>
      </thread>
      <tbody>
        <% @cuepoints.each do |cuepoint| %>
          <tr>
            <td><%= link_to cuepoint.id, edit_cuepoint_path(cuepoint) %></td>
            <td><%= cuepoint.name %></td>
            <td><%= cuepoint.created_at.in_time_zone("Tokyo") %></td>
            <td><%= cuepoint.updated_at.in_time_zone("Tokyo") %></td>
          </tr>
        <% end %>
      </tbody>
    </table>